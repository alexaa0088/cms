<% @page_title = "Admin Login" %>


<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <div class="login">
    <%= form_tag(:action => 'attempt_login') do %>
    <%= label_tag(:username) %>
    <%= text_field_tag(:username) %>
    <%= label_tag(:password) %>
    <%= password_field_tag(:password) %>
    <%= submit_tag("Log In") %>
    <% end %>
  </div>
</div>
</div>
